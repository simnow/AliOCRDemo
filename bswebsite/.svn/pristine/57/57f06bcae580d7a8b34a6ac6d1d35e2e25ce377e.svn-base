        //   $('.main-title').css('width', 1960 + (1960-$('.body').width())*2 + 'px');
        //   $('.main-nav').css('width', 1960 + (1960-$('.body').width())*2 + 'px');
        // }
       
        $(".main-nav-list li p").hover(function(){
          $(this).css("background-color","#fff");
          $(this).css("color","rgba(18, 106, 204, 1)");
        },function(){
          $(this).css("background-color","rgba(18, 106, 204, 1)");
          $(this).css("color","#fff");
        });
        $(".nav-lists").hover(function(){
          $(this).find('ul').show();
        },function(){
          $(this).find('ul').hide();
        });
        $(".make-index").on('click', function() {
          show_index(window.location);
        });
        $(".collection").on('click', function() {
          show_Favorite(window.location, document.title);
        });
        $(".input-box .btn").on('click', function() {
          if (path.indexOf("新闻列表") != '-1') {
            if ($(".input-box .searches").val().length <= 0) {
              window.location.href = encodeURI( "新闻列表.html");
            } else {
              window.location.href = encodeURI( "新闻列表.html?search=" + $(".input-box .searches").val());
            }
          } else {
            if ($(".input-box .searches").val().length <= 0) {
              return;
            }
            window.location.href = encodeURI( "新闻列表.html?search=" + $(".input-box .searches").val());
          }
        
        });
    });

    function show_index(url) {      
      if (document.all) { 
        document.body.style.behavior = 'url(#default#homepage)';        
        document.body.setHomePage(url);         
      } else {
        alert("您好,您的浏览器不支持自动设置页面为首页功能,请您手动在浏览器里设置该页面为首页!");       
      }   
    } 

    //加入收藏  
    function show_Favorite(sURL, sTitle) {     
      sURL = encodeURI(sURL);  
      try { 
        window.external.addFavorite(sURL, sTitle);
      }catch (e) {         
        try {
          window.sidebar.addPanel(sTitle, sURL, ""); 
        }catch (e) {
          alert("加入收藏失败,请使用Ctrl+D进行添加,或手动在浏览器里进行设置.");
        }        
      }    
	  } 
