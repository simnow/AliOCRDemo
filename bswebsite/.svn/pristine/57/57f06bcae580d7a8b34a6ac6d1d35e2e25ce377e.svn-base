  <script>

  	$(function(){
        var testUrl = window.location;
        var path = decodeURI(window.location);
        if (path.indexOf("新闻列表") != '-1') {
          var urls = window.location.search;
          var loc =  decodeURI(urls.substring(urls.lastIndexOf('=')+1, urls.length));
          console.log('222222', loc);
          $(".input-box .searches").val(loc);
        }
        $(".main-nav-list li p").on('click',function() {
          var url = $(this).text();
          if (url == '首页') {
            url = 'index';
          } else if (url == '咨询研究') {
            url = '战略咨询';
          } else if (url == '关于我们') {
            url = '战略院简介';
          } else if (url == '资源优势') {
            url = '专家库';
          } else if (url == '核心业务') {
            url = '战略咨询';
          } else if (url == '动态资讯') {
            url = '社会热点';
          } else if (url == '加入我们') {
            url = '联系我们';
          } 
          window.location.href = url +'.html';
        });
        if (1960-$('.body').width() > 0 && 1960-$('.body').width() > 658) {
          $(".header").css('width', 1660 +'px');
        } 
