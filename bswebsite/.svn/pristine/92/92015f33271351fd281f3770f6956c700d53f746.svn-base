  <script>
  function intro(name){
	  window.location.href = encodeURI( "组织架构人员简介.html?name=" + name+"&unit="+"学术委员会");
  }
  //	上来先展开
  $('#nav-box .nav1').click(function(){
	  //转换页面
	  window.location.href='战略院简介.html';
  });
 $('#nav-box .nav3').click(function(){
	  //转换页面
	  window.location.href='战略院大事记.html';
  });

  $('#nav-box .nav').click(function(){
	  //转换页面
	  window.location.href='组织架构.html';
  });

    //分页
    var page = 1;
    $(".pre").click(function(){
      if(page == 3) {
        $("#third-page").hide();
        $("#second-page").show();
        $("#first-page").hide();
        $(".showpage").text('第2页');
        $(".first-page").css('color', '#000');
        $(".second-page").css('color', "rgba(18, 106, 204, 1)");
        $(".third-page").css('color', "#000");
        page -- ;
      } else if(page == 2) {
        $("#third-page").hide();
        $("#second-page").hide();
        $("#first-page").show();
        $(".showpage").text('第1页');
        $(".first-page").css('color', 'rgba(18, 106, 204, 1)');
        $(".second-page").css('color', "#000");
        $(".third-page").css('color', "#000");
        page -- ;
      } else {
        return;
      }
    });
    $(".next").click(function(){
      if(page == 2) {
        $("#third-page").show();
        $("#second-page").hide();
        $("#first-page").hide();
        $(".showpage").text('第3页');
        $(".first-page").css('color', '#000');
        $(".second-page").css('color', "#000");
        $(".third-page").css('color', "rgba(18, 106, 204, 1)");
        page ++;
      } else if(page == 1) {
        $("#third-page").hide();
        $("#second-page").show();
        $("#first-page").hide();
        $(".showpage").text('第2页');
        $(".first-page").css('color', '#000');
        $(".second-page").css('color', "rgba(18, 106, 204, 1)");
        $(".third-page").css('color', "#000");
        page ++ ;
      } else {
        return;
      }
    });

    $(".tofirst").click(function() {
      $("#second-page").hide();
      $("#first-page").show();
      $("#third-page").hide();
      $(".showpage").text('第1页');
      $(".first-page").css('color', 'rgba(18, 106, 204, 1)');
      $(".second-page").css('color', "#000");
      $(".third-page").css('color', "#000");
      page = 1;
    });
    $(".tosecond").click(function() {
      $("#second-page").show();
      $("#first-page").hide();
      $("#third-page").hide();
      $(".showpage").text('第2页');
      $(".second-page").css('color', 'rgba(18, 106, 204, 1)');
      $(".first-page").css('color', "#000");
      $(".third-page").css('color', "#000");
      page = 2;
    });
    $(".tothird").click(function() {
      $("#first-page").hide();
      $("#second-page").hide();
      $("#third-page").show();
      $(".showpage").text('第3页');
      $(".second-page").css('color', '#000');
      $(".first-page").css('color', "#000");
      $(".third-page").css('color', "rgba(18, 106, 204, 1)");
      page = 3;
    });
   
  </script>
