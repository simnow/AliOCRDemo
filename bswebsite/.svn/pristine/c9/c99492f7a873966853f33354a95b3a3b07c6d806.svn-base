  	$(function(){
  		$('#nav-box .nav').click(function(){
  			$('#nav-box .nav').each(function() {
	  			
	  			$(this).find('p').css('color', '#126ACC');
	  			$(this).find('div').css({

	  				'webkitTransform':'rotate(0deg)',

	  				'transform':'rotate(0deg)'
	  				});
	  		});
  			$(this).find('div').css({

  				'webkitTransform':'rotate(90deg)',

  				'transform':'rotate(90deg)'
  				});
  			$(this).next('li').find('ul').find('p').css('color', '#666666');
  			console.log($(this).next('li').html());
  			if ($(this).next('li').is(':hidden')) {
  				$('#nav-box .sub-nav').each(function() {
  	  				$(this).hide();
  	  			});
  				$(this).next('li').show();
  			} else {
  				$(this).next('li').hide();
  				$(this).find('div').css({

	  				'webkitTransform':'rotate(0deg)',

	  				'transform':'rotate(0deg)'
	  				});
  			}
  		});
  		
  		$('#nav-box .sub-nav li').click(function(){
  			$('#nav-box .sub-nav li').each(function() {
	  			$(this).find('p').css('color', '#666666');
	  		});
  			$(this).find('p').css('color', '#126ACC');
  		});
  	});
  </script>
