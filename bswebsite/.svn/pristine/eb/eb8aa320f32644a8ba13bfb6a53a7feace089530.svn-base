  <script>
  //	上来先展开
  	$(function(){
      var locse = '';
      if(decodeURI(window.location).indexOf("?") != '-1') {
        var urlse = window.location.search;
        locse =  decodeURI(urlse.substring(urlse.lastIndexOf('=')+1, urlse.length));
        $(".search-people .searches").val(locse);
      } else {
        locse =  '';
      }
      var data = {
        name: locse,
        pageNo: 1,
        pageSize: 15
      };
      $.get('/bswebsite/api/person/name', data, function (json) {
        console.log(json);
        if (json.success) {
          var listData = json.attributes.person;
          var lists = '';
          for (var i in listData) {
            lists += '<li>';
              lists += '<div class="photo">';
                lists += '<img src="'+  listData[i]['pictureurl'] +'">';
              lists += '</div>';
              lists += '<div class="people-info">';
                lists += '<p class="name"> '+ listData[i]['name'] + '</p>';
                lists += '<p class="position">职务职称：<span>' + listData[i]['zwzc'] + '</span></p>';
                lists += '<p class="major">专业方向：<span> '+ listData[i]['zyfx'] + '</span></p>';
              lists += '</div>';
            lists += '</li>';
          }
          $("#content-box ul").html(lists);
          $("#content-box ul li").on('click', function() {
            var name = $(this).find('.name').text();
            window.location.href = encodeURI( "专家简介.html?name=" + name);
          });
        }
          
      });

      $("#nav-box .na ").on('click',function() {
        var url = $(this).children('p:first-child').text();
